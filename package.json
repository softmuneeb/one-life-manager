{
  "name": "barakah-tracker",
  "version": "1.0.0",
  "description": "BarakahTracker - A comprehensive daily tracking system with WhatsApp integration and web dashboard for monitoring target vs actual activities",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "postinstall": "npm run build",
    "check-env": "ts-node src/index.ts check-env",
    "demo": "ts-node demo-setup.ts",
    "demo-local": "ts-node demo-local.ts",
    "demo:build": "npm run build && node dist/demo-setup.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "chatbot",
    "whatsapp",
    "timetable",
    "reminder",
    "scheduler"
  ],
  "author": "Muneeb Zubair Khan",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.24",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@types/qrcode-terminal": "^0.12.2",
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.2",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "moment": "^2.30.1",
    "mongoose": "^8.19.3",
    "node-cron": "^3.0.3",
    "qrcode-terminal": "^0.12.0",
    "whatsapp-web.js": "^1.23.0"
  }
}
